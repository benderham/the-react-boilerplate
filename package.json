    "gulp-autoprefixer": "^3.1.0",
    "gulp-combine-media-queries": "^0.2.0",
    "gulp-cssnano": "^2.1.0",
    "gulp-imagemin": "^2.4.0",
    "gulp-jade": "^1.1.0",
    "gulp-modernizr": "0.0.0",
    "gulp-notify": "^2.0.0",
    "gulp-plumber": "^1.0.1",
    "gulp-sass": "^2.1.1",
    "gulp-size": "^2.0.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.1",
    "gulp-util": "^3.0.1",
    "imagemin-pngquant": "^4.2.0",
    "pretty-hrtime": "^1.0.1",
    "require-dir": "^0.3.0",
    "run-sequence": "^1.1.5",
    "vinyl-source-stream": "^1.0.0",
    "watchify": "^3.4.0"
