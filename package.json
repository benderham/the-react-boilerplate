{
  "name": "the-react-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate for a react application.",
  "main": "index.js",
  "scripts": {
    "start": "watchify src/main.jsx -v -t [ babelify --presets [ react ] ] -o public/js/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benderham/the-react-boilerplate.git"
