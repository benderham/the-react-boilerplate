  "bugs": {
    "url": "https://github.com/benderham/the-react-boilerplate/issues"
  },
  "homepage": "https://github.com/benderham/the-react-boilerplate#readme",
  "dependencies": {
    "react": "^0.14.7",
    "react-dom": "^0.14.7"
  },
  "browser": {
    "jquery": "./bower_components/jquery/dist/jquery.js"
  },
  "browserify-shim": {
    "jquery": "$"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
